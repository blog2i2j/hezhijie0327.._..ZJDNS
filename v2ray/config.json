{
    "log": {
        "loglevel": "error"
    },
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 443,
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "alterId": 0,
                        "id": "99235a6e-05d4-2afe-2990-5bc5cf1f5c52",
                        "security": "auto"
                    }
                ]
            },
            "streamSettings": {
                "acceptProxyProtocol": true,
                "earlyDataHeaderName": "Sec-WebSocket-Protocol",
                "headers": {
                    "Host": "demo.zhijie.online"
                },
                "maxEarlyData": 4096,
                "network": "ws",
                "path": "/",
                "security": "tls",
                "sockopt": {
                    "tcpFastOpen": true
                },
                "tlsSettings": {
                    "allowInsecure": false,
                    "allowInsecureCiphers": false,
                    "alpn": [
                        "h2",
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/etc/v2ray/cert/fullchain.cer",
                            "keyFile": "/etc/v2ray/cert/zhijie.online.key",
                            "usage": "encipherment"
                        }
                    ],
                    "serverName": "demo.zhijie.online"
                },
                "useBrowserForwarding": true
            }
        }
    ],
    "outbounds": [
        { "mux": { "concurrency": 8, "enable": true }, "protocol": "freedom" }
    ]
}
