            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "acceptProxyProtocol": true,
                    "earlyDataHeaderName": "",
                    "headers": {
                        "Host": "demo.zhijie.online"
