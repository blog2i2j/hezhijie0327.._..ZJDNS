                    {
                        "id": "99235a6e-05d4-2afe-2990-5bc5cf1f5c52",
                        "level": 0
                    }
                ],
                "decryption": "none",
                "fallbacks": [
                    {
                        "dest": 7890
                    },
                    {
                        "dest": 2053,
                        "path": "/wss-shadowsocks",
                        "xver": 2
                    },
                    {
                        "dest": 2083,
                        "path": "/wss-trojan",
