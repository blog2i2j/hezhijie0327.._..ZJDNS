{
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 443,
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "alterId": 0,
                        "id": "99235a6e-05d4-2afe-2990-5bc5cf1f5c52",
                        "security": "auto"
                    }
                ],
                "disableInsecureEncryption": true
            },
            "streamSettings": {
                "network": "ws",
                "security": "tls",
                "sockopt": {
                    "mark": 0,
                    "tcpFastOpen": true,
                    "tcpFastOpenQueueLength": 4096,
                    "tcpKeepAliveInterval": 0,
                    "tproxy": "tproxy"
                },
                "tlsSettings": {
                    "allowInsecure": false,
                    "alpn": [
                        "h2",
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/etc/v2ray/cert/fullchain.cer",
                            "keyFile": "/etc/v2ray/cert/zhijie.online.key",
                            "usage": "encipherment"
                        }
                    ],
                    "disableSystemRoot": false,
                    "serverName": "demo.zhijie.online",
                    "verifyClientCertificate": false
                },
                "wsSettings": {
                    "acceptProxyProtocol": false,
                    "earlyDataHeaderName": "",
                    "headers": {
                        "Host": "demo.zhijie.online"
                    },
                    "maxEarlyData": 4096,
                    "path": "/",
                    "pinnedPeerCertificateChainSha256": "",
                    "useBrowserForwarding": true
                }
            }
        }
    ],
    "outbounds": [
        { "mux": { "concurrency": 8, "enable": true }, "protocol": "freedom" }
    ]
}
