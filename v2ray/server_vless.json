{
    "log": {
        "access": "none",
        "loglevel": "info"
    },
    "dns": {
        "disableCache": true,
        "disableFallback": false,
        "queryStrategy": "UseIP",
        "servers": [
            { "address": "127.0.0.1", "port": 53 } 
        ]
    },
    "routing": {
        "domainMatcher": "mph",
        "domainStrategy": "IPOnDemand",
        "rules": [
            {
                "ip": [
                    "0.0.0.0/8",
                    "10.0.0.0/8",
                    "100.64.0.0/10",
                    "127.0.0.0/8",
                    "169.254.0.0/16",
                    "172.16.0.0/12",
                    "192.0.0.0/24",
                    "192.0.2.0/24",
                    "192.31.196.0/24",
                    "192.52.193.0/24",
                    "192.88.99.0/24",
                    "192.168.0.0/16",
                    "192.175.48.0/24",
                    "198.18.0.0/15",
                    "198.51.100.0/24",
                    "203.0.113.0/24",
                    "240.0.0.0/4",
                    "::/127",
                    "64:ff9b::/96",
                    "64:ff9b:1::/48",
                    "100::/64",
                    "2001::/23",
                    "2001:db8::/32",
                    "2002::/16",
                    "2620:4f:8000::/48",
                    "fc00::/7",
                    "fe80::/10"
                ],
                "outboundTag": "direct",
                "type": "field"
            }
        ]
    },
    "inbounds": [
        {
            "port": 443,
            "listen": "0.0.0.0",
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "99235a6e-05d4-2afe-2990-5bc5cf1f5c52"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "grpcSettings": {
                    "serviceName": "gRPCVLESS"
                },
                "network": "grpc",
                "security": "tls",
                "tlsSettings": {
                    "alpn": [
                        "h2",
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/etc/v2ray/cert/fullchain.cer",
                            "keyFile": "/etc/v2ray/cert/zhijie.online.key",
                            "usage": "encipherment"
                        }
                    ],
                    "disableSystemRoot": false,
                    "verifyClientCertificate": false
                }
            }
        },
        {
            "listen": "0.0.0.0",
            "port": 7890,
            "protocol": "http",
            "settings": {
                "allowTransparent": false
            }
        },
        {
            "listen": "0.0.0.0",
            "port": 7891,
            "protocol": "socks",
            "settings": {
                "udp": true
            }
        }
    ],
    "outbounds": [
        { "mux": { "concurrency": 8, "enabled": false }, "protocol": "socks", "settings": { "servers": [ { "address": "127.0.0.1", "port": 40000 } ] }, "tag": "warp" },
        { "mux": { "concurrency": 8, "enabled": false }, "protocol": "freedom", "tag": "direct" }
    ]
}
