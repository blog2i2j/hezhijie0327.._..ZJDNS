:443 {
    @wss {
        header Connection Upgrade
        header Upgrade websocket
    }
    handle /wss-trojan {
        reverse_proxy @wss localhost:7891 /wss-trojan
    }
    handle /wss-vmess {
        reverse_proxy @wss localhost:7892 /wss-vmess
    }
    tls /etc/caddy/cert/fullchain.cer /etc/caddy/cert/zhijie.online.key
}
